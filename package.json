{
  "name": "@vtabary/unprepared-music",
  "description": "An application to play sounds fastly",
  "author": "Vincent Tabary",
  "repository": {
    "type": "git",
    "url": "https://github.com/vtabary/unprepared-music"
  },
  "browser": {
    "fs": false,
    "path": false,
    "util": false
  },
  "homepage": "https://github.com/vtabary/unprepared-music",
  "license": "ISC",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --base-href \"\"",
    "build": "ng build --base-href \"\"",
    "build:prod": "ng build --base-href \"\" --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "main": "electron.js",
  "build": {
    "appId": "com.vtabary.unprepared-music",
    "productName": "Unprepared music",
    "copyright": "Copyright Â© 2019 Vincent Tabary",
    "files": [
      "dist/unprepared-music",
      "electron.js"
    ],
    "directories": {
      "buildResources": "./build",
      "output": "./dist/packages/${os}/${arch}"
    },
    "win": {
      "icon": "./build/icon.png",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        },
        {
          "target": "portable",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "nsis": {}
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.9",
    "@angular/common": "~8.2.9",
    "@angular/compiler": "~8.2.9",
    "@angular/core": "~8.2.9",
    "@angular/forms": "~8.2.9",
    "@angular/platform-browser": "~8.2.9",
    "@angular/platform-browser-dynamic": "~8.2.9",
    "@angular/router": "~8.2.9",
    "@clr/angular": "^2.2.2",
    "@clr/icons": "2.2.2",
    "@clr/ui": "2.2.2",
    "@webcomponents/custom-elements": "^1.0.0",
    "ngx-electron": "^2.1.1",
    "recursive-readdir": "^2.2.2",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.9",
    "@angular/cli": "~8.3.9",
    "@angular/compiler-cli": "~8.2.9",
    "@angular/language-service": "~8.2.9",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.7.12",
    "codelyzer": "^5.0.0",
    "electron": "^9.1.0",
    "electron-builder": "^21.2.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  }
}
